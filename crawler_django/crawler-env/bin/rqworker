#!/home/amirsorouri00/Desktop/opt/search-engine/distributed-django-SE/crawler_django/crawler-env/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'rq==1.0','console_scripts','rqworker'
__requires__ = 'rq==1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rq==1.0', 'console_scripts', 'rqworker')()
    )
